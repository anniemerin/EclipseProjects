package exceptions;

public class UncheckedExceptionMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		UncheckedException ex=new UncheckedException();
		try {
			
			ex.division();
			
		}
		catch (Exception e) {
			
			System.out.println("Exception : "+e);
		}
		

	}

}
