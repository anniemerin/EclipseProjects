package collections;
import java.util.ArrayList;

public class ArrayListMulti {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Create an ArrayList without specifying a data type (raw type)
        ArrayList list = new ArrayList();
        
        System.out.println("number of elements "+list.size());
        // Add elements of different types to the list
        list.add("Hello");  // String
        list.add(123);       // Integer
        list.add(3.14);      // Double
        list.add(true);      // Boolean
        
        System.out.println("number of elements "+list.size());
        
        // Access elements of the list
        for (Object obj : list) {
            System.out.println(obj);
        }
        
        // inserting element 
        list.add(2,"training");  //2 describes after number of elements not position
        
     // Access elements of the list
        for (Object obj : list) {
            System.out.println(obj);
        }
        
        
	}

}
